{
  "name": "vitra",
  "version": "0.1.1",
  "description": "A framework for cooperatively-run databases using smart contracts.",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "vitra": "./bin.js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "docs": "typedoc --readme ./docs/api-reference-readme.md --out ./generated-docs src/index.ts --excludedFunctionOrMethod \"^_\" && gh-pages -d ./generated-docs && rm -Rf ./generated-docs",
    "test": "ava -s test/*.ts"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "author": "Paul Frazee <pfrazee@gmail.com>",
  "license": "",
  "dependencies": {
    "@babel/parser": "^7.16.8",
    "@hyperswarm/dht": "^5.0.12",
    "chalk": "^4.1.2",
    "compact-encoding": "^2.5.1",
    "confine-sandbox": "^0.3.3",
    "core-js-pure": "^3.20.1",
    "corestore": "^6.0.1-alpha.9",
    "event-iterator": "^2.0.0",
    "frame-stream": "^3.0.0",
    "hyperbee": "^1.7.0",
    "hypercore": "^10.0.0-alpha.19",
    "hyperswarm": "^3.0.1",
    "jsonrpc-lite": "^2.2.0",
    "lodash.isequal": "^4.5.0",
    "minimist": "^1.2.5",
    "msgpackr": "^1.5.2",
    "pump": "^3.0.0",
    "random-access-file": "^2.2.0",
    "random-access-memory": "^4.0.0",
    "stream-to-it": "^0.2.4",
    "streamx": "^2.12.0",
    "vitra-confine-runtime": "^0.5.1",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "@types/lodash.isequal": "^4.5.5",
    "@types/minimist": "^1.2.2",
    "@types/node": "^16.6.2",
    "@types/pump": "^1.1.1",
    "@types/streamx": "^2.9.1",
    "ava": "^3.15.0",
    "gh-pages": "^3.2.3",
    "ts-node": "^10.2.1",
    "typedoc": "^0.22.10",
    "typedoc-plugin-reference-excluder": "^1.0.0",
    "typescript": "^4.3.5"
  }
}
